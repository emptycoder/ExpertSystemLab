<Window x:Class="ExpertSystemUI.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExpertSystemUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel LastChildFill="True">
        <StackPanel HorizontalAlignment="Right" Orientation="Horizontal" DockPanel.Dock="Top">
            <Button Content="My experience" Click="MyExperience_OnClick" />
            <Button Content="Load rules from file" Click="LoadRules_OnClick" />
        </StackPanel>
        <Frame x:Name="Frame" />
    </DockPanel>
</Window>
